#include <printing.hpp>

namespace ohe_framework
{
void printErrorMessage(const char* str)
{
    std::cout<<KRED<<str<<KNRM<<std::endl;
}
void printResultMessage(const char* str)
{
    std::cout<<KGRN<<str<<KNRM<<std::endl;
}
void printDebugMessage(const char* str)
{
    std::cout<<KBLU<<str<<KNRM<<std::endl;
}
void printMessage(const char* str)
{
    std::cout<<str<<std::endl;
}
}
